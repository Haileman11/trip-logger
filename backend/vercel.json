{
    "version": 2,
    "builds": [
        {
            "src": "trip_logger/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "trip_logger/wsgi.py"
        }
    ]
} 