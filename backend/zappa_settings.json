{
    "production": {
        "aws_region": "us-east-1",
        "django_settings": "backend.settings",
        "profile_name": "default",
        "project_name": "trip-logger",
        "runtime": "python3.12",
        "s3_bucket": "zappa-trip-logger",
        "timeout_seconds": 900,
        "manage_roles": false,
        "role_name": "ZappaLambdaRole",
        "vpc_config": {
            "SubnetIds": [],
            "SecurityGroupIds": []
        },
        "exclude": [
            "*.pyc",
            "*.pyo",
            "*.pyd",
            ".Python",
            "env",
            "pip-log.txt",
            "pip-delete-this-directory.txt",
            ".tox",
            ".coverage",
            ".coverage.*",
            ".cache",
            "nosetests.xml",
            "coverage.xml",
            "*.cover",
            "*.log",
            ".pytest_cache",
            ".env",
            ".venv",
            "venv",
            "ENV",
            "node_modules",
            "frontend"
        ]
    }
} 